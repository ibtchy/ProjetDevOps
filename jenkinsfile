pipeline {
  agent any
       stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
                git branch: 'mahdi',
                credentialsId: 'cred',
                url: 'https://github.com/ibtchy/ProjetDevOps.git';
                
            }
        }
        stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        stage('compile'){
            steps {
                sh 'mvn compile -e'
            }
        }
        stage('package'){
            steps {
                sh 'mvn clean package -DskipTests'
            }
        } 
        }
}
