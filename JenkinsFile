
pipeline {
    agent any
    
   
    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'salma', credentialsId: 'jenkinks_cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
            }
        }
        stage ('mvn Version'){
            steps {
                 sh 'mvn -version'
            }
        }
         stage ('mvn Compile'){
            steps {
                 sh 'mvn compile -e'
            }
        }
        
        stage('Package') {
        steps {
        sh('mvn clean package -DskipTests')
         }
        }
        
         stage('Test') {
         steps {
        sh(script: './mvnw --batch-mode -Dmaven.test.failure.ignore=true test')

      }
      }
    }
}
