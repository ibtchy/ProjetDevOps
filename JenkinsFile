
pipeline {
    agent any
    
   
    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'salma', credentialsId: 'jenkinks_cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
            }
        }
        stage ('mvn Version'){
            steps {
                 sh 'mvn -version'
            }
        }
         stage ('mvn Compile'){
            steps {
                 sh 'mvn compile -e'
            }
        }
        
        stage('Package') {
        steps {
        sh('mvn clean package -DskipTests')
         }
        }

        stage("scan"){
        steps {
                withSonarQubeEnv ( installationName: 'SonarQube'){
                sh 'mvn clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.1.2184:sonar'
                }
        }
        }
    }
}
