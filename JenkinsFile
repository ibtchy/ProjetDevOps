
pipeline {
    agent any
    
   
    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'salma', credentialsId: 'jenkinks_cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
            }
        }

         stage ('mvn Compile'){
            steps {
                 sh 'mvn compile -e'
            }
        }


       /* stage ('test'){
        steps{
            sh  'mvn test'
        }
        }*/


        stage("SonarQube"){
        steps {
                withSonarQubeEnv ( installationName: 'SonarQube'){
                sh 'mvn sonar:sonar'
                }
        }
        }

        stage ('Nexus'){
                steps{
                  nexusArtifactUploader
                  credentialsId: '2bf65efe-65ea-4ecb-a4f6-a1f4b217c32f', groupId: 'tn.esprit.spring', nexusUrl: 'http://192.168.1.9:8081/', nexusVersion: 'nexus3', protocol: 'http', repository: 'maven-snapshots', version: '1.0-SNAPSHOT'
                }
        }

    }
}
