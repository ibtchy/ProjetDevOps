
pipeline {
    agent any

    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'oussamabouguerra', credentialsId: 'credjenkins', url: 'https://github.com/ibtchy/ProjetDevOps.git'
                
            }
        }
         stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        stage('Compile'){
            steps {
                sh 'mvn compile -e'
            }
        }
         
       
           
        
        
        
        
    }
}
