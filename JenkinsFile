
pipeline {
    agent any
    
   
    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'salma', credentialsId: 'jenkinks_cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
            }
        }

         stage ('mvn Compile'){
            steps {
                 sh 'mvn compile -e'
            }
        }


        stage ('test'){
        steps{
            sh('mvn test')
        }
        }
        stage("SonarQube"){
        steps {
                withSonarQubeEnv ( installationName: 'SonarQube'){
                sh 'mvn sonar:sonar'
                }
        }
        }

        stage ('Nexus'){
                steps{
                    sh('mvn deploy -DskipTests')
                }
        }

    }
}
