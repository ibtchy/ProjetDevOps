pipeline {
    agent any

    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'main', credentialsId: 'Cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
                
            }
        }
      stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        
          stage('Compiling the artifact') {
             
             
            steps {
echo "compiling"
                sh 'mvn compile'
               
            }
        }
      
         
       
           
        
        
        
        
    }
}
