      stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        
          stage('Compiling the artifact') {
             
             
            steps {
echo "compiling"
                sh 'mvn compile'
               
            }
        }
        stage('Code Quality Check via SonarQube') {
             
             
            steps {
               


sh 'mvn sonar:sonar \
  -Dsonar.projectKey=cherif \
  -Dsonar.host.url=http://192.168.1.17:9000 \
  -Dsonar.login=84a2cbab2d13cac2ac9678cbaef2cad898f4c592'

 
}
               
            
        }
        stage ('Test Unitaire')
        {
        steps {
        sh 'mvn test -Dtest="tn.esprit.spring.FournisseurServiceTest.java"' 
        }
        
        }
        stage('Nexus')
       {
         steps{
         sh 'mvn deploy -DskipTests'
         }       
       }
      
         
       
           
        
        
        
        
    }
