pipeline {
    agent any

    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling'
               git branch: 'medcheriflakhoua', credentialsId: 'Cred', url: 'https://github.com/ibtchy/ProjetDevOps.git'
                
            }
        }
      stage('Maven Version'){
            steps {
                sh 'mvn -version'
            }
        }
        
        
          stage('Compiling the artifact') {
             
             
            steps {
echo "compiling"
                sh 'mvn compile'
               
            }
        }
        stage('Code Quality Check via SonarQube') {
             
             
            steps {
               
script {

sh '  mvn sonar:sonar -Dsonar.sources=src/main/java -Dsonar.css.node=. -Dsonar.java.binaries=. -Dsonar.host.url=http://http://192.168.1.17:9000/ -Dsonar.login=admin   -Dsonar.password=sonar'

 
}
               
            }
        }
      
         
       
           
        
        
        
        
    }
}
